/* -------------------- comments Pingbuller-news  ---------------------------- */
.wrapper-comments {
    width: 100%;
    height: 396px;
    background: #ececec;
}

.comments {
    width: 960px;
    margin: 0 auto;
}

.comments__buller, .comments__recent, .comments__news-a {
    display: inline-block;
    float: left;
    overflow: hidden;
    width: 280px;
    height: 200px;
    margin-top: 95px;
}

.comments__buller, .comments__recent {
    margin-right: 58px;
}

.comments__recent p, .comments__news-a p {
    margin-top: 10px;
}

.comments__buller h2 {
    margin-top: -8px;
    text-transform: uppercase;
    font-family: "League Gothic Regular", Arial, sans-serif;
    font-size: 48px;
    line-height: 55px;
    font-weight: 400;
    letter-spacing: -0.005em;
    text-shadow: 0 1px 0 #fff;
    color: #454545;
}

.comments__buller h2 span {
    text-transform: uppercase;
    font-family: "League Gothic Regular", Arial, sans-serif;
    font-size: 48px;
    line-height: 34px;
    text-shadow: 0 1px 0 #fff;
    font-weight: 400;
    color: #b9b9b9;
}

.comments__buller i {
    padding-top: 50px;
    font-family: Georgia, serif;
    font-size: 16px;
    line-height: 36px;
    font-style: italic;
    text-shadow: 1px 1px 0 #fff;
    color: #c5c5c5;
}

.comments__recent h2, .comments__news-a h2 {
    margin-top: 2px;
    text-transform: uppercase;
    font-family: "League Gothic Regular", Arial, sans-serif;
    font-size: 30px;
    line-height: 24px;
    font-weight: 400;
    text-shadow: 1px 1px 0 #fff;
    color: #454545;
}

.comments__recent p, .comments__news-a p, .comments__buller p {
    font-family: "Open Sans Light", Arial, sans-serif;
    font-size: 16px;
    line-height: 24px;
    font-weight: 300;
    letter-spacing: -0.035em;
    hyphens: auto;
    text-shadow: 1px 1px 0 #fff;
    color: #787878;
}

.comments__news-b, .comments__news-c {
    display: none;
}

/* ----------------------- footer ------------------------------------------- */
.bottom-wrapper {
    width: 100%;
    height: 113px;
    background: hsl(292, 12%, 62%) url(../img/bg-footer.jpg) no-repeat top;
}

.bottom-nav {
    display: inline-block;
    width: 528px;
    margin-top: 38px;
}

.bottom-nav li {
    margin-right: 42px;
}

.bottom-nav ul li a {
    font-size: 20px;
}

.bottom p {
    display: inline-block;
    float: right;
    margin-top: 52px;
    font-family: "Open Sans Light", Arial, sans-serif;
    font-size: 12px;
    text-shadow: #000 1px 1px 3px;
    color: #fff;
}

.bottom p strong {
    font-family: "Open Sans Extrabold", Arial, sans-serif;
    font-weight: bold;
}
